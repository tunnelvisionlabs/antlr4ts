			"grammar T;\n" +
			"a : A {System.out.println($A.text);} ;\n" +
			"A : ('a'|B) ;\n" +  // this doesn't collapse to set but works
			"fragment\n" +
			"B : ~('a'|'c') ;\n";
		let found: string =  execParser("T.g4", grammar, "TParser", "TLexer",
