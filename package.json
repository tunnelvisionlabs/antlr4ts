{
  "name": "antlr4ts",
  "version": "0.3.0-SNAPSHOT",
  "description": "Antlr4 runtime for JavaScript written in Typescript",
  "main": "index.js",
  "scripts": {
    "clean": "git clean -d -i -x",
    "antlr4ts": "antlr4ts src/tree/xpath/XPathLexer.g4 -DbaseImportPath=../.. -o src/tree/xpath && npm run antlr4ts-benchmark",
    "tsc": "tsc",
    "prepublish": "npm i tool && npm run antlr4ts && npm run tsc",
    "test": "mocha",
    "precover": "npm run prepubish",
    "cover": "istanbul cover node_modules/mocha/bin/_mocha",
    "postcover": "istanbul report",
    "dev-test-watch": "mocha-typescript-watch",
    "typedoc": "typedoc --ignoreCompilerErrors --out doc src --name antlr4ts",
    "antlr4ts-benchmark": "npm run antlr4ts-test-std && npm run antlr4ts-test-std-atn && npm run antlr4ts-test-lr && npm run antlr4ts-test-lr-atn",
    "antlr4ts-test-std": "antlr4ts -visitor src/benchmark/Java.g4 -DbaseImportPath=../../.. -o src/benchmark/gen/std",
    "antlr4ts-test-std-atn": "antlr4ts -visitor -Xforce-atn src/benchmark/Java.g4 -DbaseImportPath=../../.. -o  src/benchmark/gen/std-atn",
    "antlr4ts-test-lr": "antlr4ts -visitor src/benchmark/JavaLR.g4 -DbaseImportPath=../../.. -o src/benchmark/gen/lr",
    "antlr4ts-test-lr-atn": "antlr4ts -visitor -Xforce-atn src/benchmark/JavaLR.g4 -DbaseImportPath=../../.. -o src/benchmark/gen/lr-atn"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sharwell/antlr4ts.git"
  },
  "keywords": [
    "ANTLR4",
    "typescript"
  ],
  "author": "Terence Parr, Sam Harwell, and Burt Harris",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/sharwell/antlr4ts/issues"
  },
  "homepage": "https://github.com/sharwell/antlr4ts#readme",
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.41",
    "istanbul": "^0.4.5",
    "mocha": "^3.1.0",
    "mocha-typescript": "^1.0.10",
    "source-map-support": "^0.4.3",
    "typedoc": "^0.5.1",
    "typescript": "^2.0.3"
  },
  "dependencies": {
    "antlr4ts-cli": "file:tool"
  },
  "files": [
    "lib",
    "src"
  ]
}
