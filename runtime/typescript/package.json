{
  "name": "antlr4ts",
  "version": "0.5.0-dev",
  "description": "ANTLR 4 runtime for JavaScript written in Typescript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "npm run antlr && tsc -p .",
    "antlr": "cd ../../src/tree/xpath && antlr4ts XPathLexer.g4 -DbaseImportPath=../.."
